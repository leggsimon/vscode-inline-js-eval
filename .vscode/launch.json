// A launch configuration that launches the extension inside a new window
{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": ["--extensionDevelopmentPath=/Users/simon.legg/Dropbox/Coding/inline-js-eval" ],
            "stopOnEntry": false
        },
        {
            "name": "Launch Tests",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": ["--extensionDevelopmentPath=/Users/simon.legg/Dropbox/Coding/inline-js-eval", "--extensionTestsPath=/Users/simon.legg/Dropbox/Coding/inline-js-eval/test" ],
            "stopOnEntry": false
        }
    ]
}
